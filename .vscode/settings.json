{
  "editor.detectIndentation": false,
  "editor.tabSize": 3,
  "editor.insertSpaces": true,
  "editor.wordWrap": "on",
  "editor.defaultFormatter": "vscode.html-language-features",
  "files.exclude": {
      "**/.git": true,
      "**/.svn": true,
      "**/.hg": true,
      "**/CVS": true,
      "**/.DS_Store": true,
      "**/Thumbs.db": true,
      "**/.gitignore": true,
      "**/.env": false,
      "**/README.md": true,
      "**/yarn.lock": true,
      "**/.dockerignore": true,
      "**/node_modules": true,
      "**/.next": true,
      "**/.editorconfig": true,
      "**/.gitattributes": true,
      "**/.prettierignore": true,
      "**/LICENSE.md": true,
  },
  "cSpell.words": [
   "boxicons",
   "buttonset",
   "checkpi",
   "colreorder",
   "customizer",
   "datatable",
   "datatables",
   "daygrid",
   "fileupload",
   "fixedcolumns",
   "fixedheader",
   "formgrid",
   "fullcalendar",
   "Gufakto",
   "homeworld",
   "inputid",
   "inputnumber",
   "inputtext",
   "inputtextarea",
   "INSTOCK",
   "ishtml",
   "kebabcase",
   "keytable",
   "lable",
   "layoutcontext",
   "lokania",
   "menubutton",
   "menucontext",
   "msisdn",
   "Msisdns",
   "octicon",
   "offcanvas",
   "opentype",
   "Pipeable",
   "popperjs",
   "primeflex",
   "primeicons",
   "primereact",
   "progressspinner",
   "radiobutton",
   "reactflow",
   "reduxjs",
   "rememberme",
   "renderscript",
   "rowgroup",
   "rowreorder",
   "scroller",
   "searchbuilder",
   "searchpanes",
   "Selamat",
   "seperator",
   "speeddial",
   "starships",
   "staterestore",
   "timegrid",
   "topbar",
   "topbarmenu",
   "topbarmenubutton",
   "unoptimized"
],
  "eslint.workingDirectories": [
    { "pattern": "apps/*/" },
    { "pattern": "packages/*/" }
 ],
 "[typescript]": {
    "editor.defaultFormatter": "vscode.typescript-language-features"
 },
 "[typescriptreact]": {
    "editor.defaultFormatter": "vscode.typescript-language-features"
 },
 "[jsonc]": {
    "editor.defaultFormatter": "vscode.json-language-features"
 }

}